{
    "compilerOptions": {
      "target": "es2020",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "../../.netlify/functions-build",
      "rootDir": "./",
      "moduleResolution": "node"
    },
    "include": ["./**/*"],
    "exclude": ["node_modules"]
  }